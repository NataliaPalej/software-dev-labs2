package lab1_exceptions;

public class ExceptionsLab1 {

	public static void main(String[] args) {
		
		testMethodA("a");
		System.out.println();
		testMethodB();
		System.out.println();
		testMethodC();
		String c = testMethodC();
		System.out.println(c);
		
		System.out.println();
		
		String d = testMethodD();
		System.out.println(d);
		
		System.out.println();
		
		

	}


public static String testMethodA(){
	try{
		return "Method A";
	}
	catch (Exception e){
		return "Catch Exception: " + e;
	}
	finally{
		return "OK from finally";
	}
}

public static String testMethodB(){
	try{
		return "Method B";
	}
	catch (Exception e){
		return "Exception: " + e;
	}
	finally{
		return "Finally section";
	}
}

public static String testMethodC(){
	try{
		return "Method C";
	}
	catch (Exception e){
		return "Exception: " + e;
	}
	finally {
		return "Finally";
	}
}


public static String testMethodD(){
	try{
		return "Method D";
	}
	catch (Exception e){
		return "Exception: " + e;
	}
	finally{
		return "Finally";
	}
}
}
